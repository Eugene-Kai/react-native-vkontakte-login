{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": false,
    "member-access": false,
    "member-ordering": false,
    "no-console": [true, "log", "error"],
    "no-implicit-dependencies": [true, "dev"],
    "object-literal-sort-keys": false
  }
}
